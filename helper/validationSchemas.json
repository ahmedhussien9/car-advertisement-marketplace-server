{
  "idSchema": {
    "$async": true,
    "type": "object",
    "properties": {
      "userId": {
        "type": "string",
        "pattern": "^[0-9a-fA-F]{24}$"
      }
    },
    "required": [
      "userId"
    ]
  },
  "userData": {
    "$async": true,
    "type": "object",
    "properties": {
      "first_name": {
        "type": "string",
        "description": "Should have required property first_name"
      },
      "last_name": {
        "type": "string",
        "description": "Should required property name last_name"
      },
      "phone_number": {
        "type": "string",
        "description": "Should have required property phone_number"
      },
      "nick_name": {
        "type": "string",
        "description": "Should have required property nick_name"
      }
    },
    "required": [
      "first_name",
      "last_name",
      "nick_name",
      "phone_number"
    ]
  },
  "product": {
    "$async": true,
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "Should have required property name"
      },
      "price": {
        "type": "number",
        "description": "Should have required property price"
      },
      "creatorId": {
        "type": "string",
        "description": "Should have required property creatorId"
      }
    }
  }
}